
@{
    ViewData["Title"] = "Detalhe";
}

<div class="card">
    <div class="card-header">
        <a asp-controller="cliente" asp-action="listar" class="btn btn-warning" style="float:right;">Voltar</a>
        @ViewData["Title"]
    </div>
    <div class="card-body">


        <div class="row">
            <div class="col-md-6">
                <label>Nome</label>
                <input type="text" class="form-control" />
            </div>
            <div class="col-md-4">
                <label>Genero</label>
                <input type="text" class="form-control" />
            </div>
            <div class="col-md-2">
                <label>Data de Nascimento</label>
                <input type="date" class="form-control" />
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-md-12">
                <input type="button" name="name" value="Salvar"  class="btn btn-success btn-block"/>
            </div>
        </div>

    </div>
</div>



