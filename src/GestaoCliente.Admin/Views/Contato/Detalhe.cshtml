@model ContatoModel
@{
    ViewData["Title"] = "Detalhe";
}

<div class="card">
    <div class="card-header">
        <a href="@Url.Content($"/contato/listar?clienteId={Model.ClienteId}")" class="btn btn-warning" style="float:right;"><i class="fa fa-angle-double-left"></i>&ensp;Voltar</a>
        @ViewData["Title"]
    </div>
    <div class="card-body">


        <div class="row">
            <div class="col-md-6">
                <label>Telefone</label>
                <input type="text" class="form-control" readonly value="@Model.Telefone" />
            </div>
            <div class="col-md-6">
                <label>Email</label>
                <input type="text" class="form-control" readonly value="@Model.Email" />
            </div>
        </div>

    </div>
</div>

